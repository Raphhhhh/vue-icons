{
  "name": "vue-icons",
  "description": "webpack based - load only what you need - svg inline icons",
  "version": "1.1.0",
  "homepage": "https://github.com/vue-comps",
  "author": {
    "name": "Paul Pflugradt",
    "email": "paul.pflugradt@gmail.com"
  },
  "license": "MIT",
  "main": "icon-font-compatible.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/vue-comps/vue-icons"
  },
  "engines": {
    "node": "*"
  },
  "dependencies": {
    "vue-mixins": "^0.2.7"
  },
  "peerDependencies": {
    "callback-loader": "^0.2.4"
  },
  "devDependencies": {
    "bluebird": "^3.3.5",
    "bootstrap": "^3.3.6",
    "callback-loader": "^0.2.4",
    "chai": "^3.5.0",
    "chai-spies": "^0.7.1",
    "coffee-loader": "^0.7.2",
    "coffee-script": "^1.10.0",
    "font-awesome": "^4.6.1",
    "gh-pages": "^0.11.0",
    "karma": "^0.13.22",
    "karma-chai": "^0.1.0",
    "karma-chai-dom": "^1.1.0",
    "karma-chai-spies": "^0.1.4",
    "karma-chrome-launcher": "^1.0.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.0.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.26",
    "karma-vue-component": "^0.1.0",
    "karma-webpack": "^1.7.0",
    "material-design-icons": "^2.2.3",
    "mdi": "^1.5.54",
    "mocha": "^2.4.5",
    "octicons": "^3.5.0",
    "open-iconic": "^1.1.1",
    "pug": "^2.0.0-alpha6",
    "script-runner": "^0.1.4",
    "svgfont2js": "^0.1.2",
    "svgo": "^0.6.6",
    "template-html-loader": "0.0.3",
    "vue": "^1.0.21",
    "vue-compiler": "^0.3.0",
    "vue-dev-server": "^0.2.10",
    "vue-html-loader": "^1.2.2",
    "vue-loader": "^8.2.3",
    "webpack": "^1.13.0"
  },
  "keywords": [
    "icon",
    "webpack",
    "component",
    "vue"
  ],
  "readmeFilename": "README.md",
  "scripts": {
    "build:coffee": "coffee --no-header --compile --output . src/icon-loader.coffee src/icon-font-compatible.coffee",
    "build:fonts": "coffee ./src/process-fonts.coffee",
    "build:vue": "NODE_ENV=production vue-compiler --out . src/*.vue",
    "build": "run-npm build:*",
    "dev": "vue-dev-server",
    "watch:coffee": "coffee --no-header --watch --output . src/icon-loader.coffee src/icon-font-compatible.coffee",
    "watch:test": "karma start --browsers Chrome --auto-watch --reporters spec",
    "watch": "run-npm watch:*",
    "test": "karma start --single-run",
    "preversion": "npm test",
    "version": "npm run build && git add .",
    "postversion": "git push && git push --tags && npm publish",
    "ghpages": "vue-dev-server --static static/ && gh-pages -d static"
  }
}
